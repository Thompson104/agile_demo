<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="demo" >

	<xacro:include filename="$(find ur_description)/urdf/ur.gazebo.xacro" />
	<xacro:include filename="$(find ur_description)/urdf/ur5.urdf.xacro" />

	<xacro:ur5_robot prefix="ur_" joint_limited="false" />

	<link name="map"></link>

	<!-- arm -->
	<joint name="robot_joint" type="fixed">
		<parent link="map" />
		<child  link="ur_base_link" />
		<origin rpy="0 0 0" />
	</joint>

</robot>
